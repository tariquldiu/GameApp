<!DOCTYPE HTML>
<html>
<head>
    <title>Unique Shop Admin</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Minimal Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <link href="~/AdminTemplate/css/bootstrap.min.css" rel='stylesheet' type='text/css' />
    <!-- Custom Theme files -->
    <link href="~/AdminTemplate/css/style.css" rel='stylesheet' type='text/css' />
    <link href="~/AdminTemplate/css/font-awesome.css" rel="stylesheet">
    <script src="~/AdminTemplate/js/jquery.min.js"></script>
    <!-- Mainly scripts -->
    <script src="~/AdminTemplate/js/jquery.metisMenu.js"></script>
    <script src="~/AdminTemplate/js/jquery.slimscroll.min.js"></script>
    <!-- Custom and plugin javascript -->
    <link href="~/AdminTemplate/css/custom.css" rel="stylesheet">
    <script src="~/AdminTemplate/js/custom.js"></script>
    <script src="~/AdminTemplate/js/screenfull.js"></script>
    <script src="~/Scripts/angular.js"></script>
    <script>
        $(function () {
            $('#supported').text('Supported/allowed: ' + !!screenfull.enabled);

            if (!screenfull.enabled) {
                return false;
            }



            $('#toggle').click(function () {
                screenfull.toggle($('#container')[0]);
            });



        });
    </script>

    <!----->
    <!--pie-chart--->
    <!--skycons-icons-->
    <script src="~/AdminTemplate/js/skycons.js"></script>
    <!--//skycons-icons-->
    @RenderSection("style", required: false)
</head>
<body>
    <div id="wrapper">

        <!----->
        <nav class="navbar-default navbar-static-top" role="navigation">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <h1> <a class="navbar-brand" href="https://bduniqueshop.com/">UNIQUE</a></h1>
            </div>
            <div class=" border-bottom">
                

                <!-- Brand and toggle get grouped for better mobile display -->
                <!-- Collect the nav links, forms, and other content for toggling -->

                <div class="clearfix">

                </div>

                <div class="navbar-default sidebar" role="navigation">
                    <div class="sidebar-nav navbar-collapse">
                        <ul class="nav" id="side-menu">


                            @if (User.Identity.IsAuthenticated)
                            {
                                <li>
                                    <a href="/Users/Index"><i class="fa fa-list nav_icon "></i><span class="nav-label">Users</span> </a>
                                </li>
                                <li>
                                    <a href="/Orders/Index"><i class="fa fa-shopping-cart nav_icon "></i><span class="nav-label">Orders</span> </a>
                                </li>
                                <li>
                                    <a href="/Products/Index"><i class="fa fa-product-hunt nav_icon "></i><span class="nav-label">Products</span> </a>
                                </li>
                                <li>
                                    <a href="/GameTopups/Index"><i class="fa fa-gamepad nav_icon "></i><span class="nav-label">Game Topups</span> </a>
                                </li>
                                <li>
                                    <a href="/Notices/Index"><i class="fa fa-book nav_icon "></i><span class="nav-label">Notices</span> </a>
                                </li>
                                <li>
                                    <a href="/PaymentGateways/Index"><i class="fa fa-money nav_icon "></i><span class="nav-label">Payment Gateways</span> </a>
                                </li>
                                <li><a href="/Users/ChangePassword"><i class="fa fa-edit nav_icon"></i><span class="nav-label">Change Password</span></a></li>
                                <li><a href="/Home/Index"><i class="fa fa-arrow-circle-left nav_icon"></i><span class="nav-label">Home Page</span></a></li>
                            }
                            @if (User.Identity.IsAuthenticated)
                            {
                                <li>
                                    <a href="#" class=" hvr-bounce-to-right">
                                        <i class="fa fa-user nav_icon"></i>
                                        @if (User.Identity.IsAuthenticated)
                                        {
                                            @User.Identity.Name
                                        }

                                    </a>
                                </li>
                            }

                            @if (User.Identity.IsAuthenticated)
                            {

                                <li><a href="/Users/Logout" class=" hvr-bounce-to-right"><i class="fa fa-sign-out nav_icon"></i>Logout</a></li>

                            }

                        </ul>
                    </div>
                </div>
        </nav>
        <div id="page-wrapper" class="gray-bg dashbard-1">
            <div class="content-main">
                <div class="banner">
                    @RenderBody()
                </div>
                <br />
                <div class="copy">
                    <p>  &copy; @DateTime.Now.Year All Rights Reserved by  <a href="https://bduniqueshop.com/">UNIQUE SHOP</a> </p>
                </div>
            </div>
            <div class="clearfix"> </div>
        </div>
    </div>
    <!---->
    <!--scrolling js-->
    <script src="~/AdminTemplate/js/jquery.nicescroll.js"></script>
    <script src="~/AdminTemplate/js/scripts.js"></script>
    <!--//scrolling js-->
    <script src="~/AdminTemplate/js/bootstrap.min.js"></script>
</body>
</html>

